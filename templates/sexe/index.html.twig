{% extends 'base.html.twig' %}

{% block title %}Sexe index{% endblock %}

{% block body %}
    <h1>Sexe index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>CreatedAt</th>
                <th>Name</th>
                <th>Code</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for sexe in sexes %}
            <tr>
                <td>{{ sexe.id }}</td>
                <td>{{ sexe.createdAt ? sexe.createdAt|date('Y-m-d H:i:s') : '' }}</td>
                <td>{{ sexe.name }}</td>
                <td>{{ sexe.code }}</td>
                <td>
                    <a href="{{ path('sexe_show', {'id': sexe.id}) }}">show</a>
                    <a href="{{ path('sexe_edit', {'id': sexe.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="5">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('sexe_new') }}">Create new</a>
{% endblock %}
